{% extends 'layout.html.twig' %}

{% block content %}
    <div class="page-header">
        <h1>Recently added projects</h1>
    </div>

    {% for project in projects %}
        <h2><a href="{{ path('project_show', { "id": project.id }) }}">{{ project.name }}</a></h2>
        <p>{{ project.description }}</p>
    {% endfor %}

{% endblock %}